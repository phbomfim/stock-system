<div>
    <h1 class="mt-3 mb-3">Locais de Armazenamento:</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Local</th>
    </tr>
  </thead>
  <tbody>
    <% @locais.each do |l| %>
        <tr>
            <th scope="row"><%= l.id %></th>
            <td><%= l.nome %></td>
        </tr>
    <% end %>
  </tbody>
</table>