<div>
    <h1 class="mt-3 mb-3">Locais de Armazenamento:</h1>
    <% @locais.each do |l| %>
        <h6><%= l.id %> - <%= l.nome %> </h6>
    <% end %>
</div>

<div>
    <h3 class="mt-3 mb-3"> Upload to database </h3>
    <%= form_with model: @local, url: up2_path do |form| %>
        <%= form.submit %>
    <% end %>
</div>